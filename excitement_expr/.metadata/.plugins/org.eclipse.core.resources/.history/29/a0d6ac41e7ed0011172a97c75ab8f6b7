package eu.excitement.entailment.core.representation.parsetree;

/**
 * Dependency Relation Types, according to the adopted UIMA dependency type. 
 * (DKPro.type.dependency.Dependency subtypes) 
 * 
 * Updating this enum must be done according to this UIMA type. 
 * @author tailblues
 *
 */
public enum DependencyRelationType {

	ABBREV,
	ACOMP,
	ADVCL,
	ADVMOD,
	AGENT,
	AMOD,
	APPOS,
	ATTR,
	AUX0,
	AUXPASS,
	CC, 
	CCOMP, 
	COMPLM, 
	CONJ, 
	CONJP, 
	CONJ_YET, 
	COP, 
	CSUBJ, 
	CSUBJPASS,
	DEP, 
	DET, 
	DOBJ,
	EXPL,
	INFMOD,
	IOBJ,
	MARK,
	MEASURE,
	MWE,
	NEG,
	NN,
	NPADVMOD,
	NSUBJ,
	NSUBJPASS,
	NUM,
	NUMBER,
	PARATAXIS,
	PARTMOD,
	PCOMP,
	POBJ,
	POSS,
	POSSESSIVE,
	PRECONJ,
	PRED,
	PREDET,
	PREP,
	PREPC,
	PRT,
	PUNCT,
	PURPCL,
	QUANTMOD,
	RCMOD,
	REF,
	REL,
	TMOD,
	XCOMP,
	XSUBJ, 
	;	
}
